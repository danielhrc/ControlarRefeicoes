<p id="notice"><%= notice %></p>

<h1>User Has Refeicaos</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Refeicao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_has_refeicaos.each do |user_has_refeicao| %>
      <tr>
        <td><%= user_has_refeicao.user_id %></td>
        <td><%= user_has_refeicao.refeicao_id %></td>
        <td><%= link_to 'Show', user_has_refeicao %></td>
        <td><%= link_to 'Edit', edit_user_has_refeicao_path(user_has_refeicao) %></td>
        <td><%= link_to 'Destroy', user_has_refeicao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Has Refeicao', new_user_has_refeicao_path %>
